{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "dev-eas": {
      "env": {
        "TYPE": "dev"
      }
    },
    "production-eas": {
      "env": {
        "TYPE": "production"
      }
    },
    "development": {
      "extends": "dev-eas",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "extends": "dev-eas",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "channel": "preview"
    },
    "production": {
      "extends": "production-eas",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
