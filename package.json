    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/react": "~18.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4"
  },
  "private": true
